{
  "name": "vulpem.com",
  "version": "3.0.0",
  "description": "Vulpem Ventures Main Website",
  "scripts": {
    "serve": "npx parcel --no-cache --port 8080 src/index.html",
    "build": "npx parcel build src/index.html",
    "prepare": "npx cpx CNAME dist && npx cpx bitcoin.pdf dist && npx cpx synthetic-asset-smart-contract.pdf dist && mkdir -p dist/.well-known && npx cpx nostr.json dist/.well-known/",
    "deploy": "yarn prepare && npx push-dir --dir=dist --branch=gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vulpemventures/vulpemcom.git"
  },
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "author": "Vulpem Ventures",
  "license": "MIT",
  "devDependencies": {
    "cpx": "^1.5.0",
    "parcel-bundler": "^1.12.4",
    "push-dir": "^0.4.1"
  },
  "dependencies": {}
}
